{
  "@context": "https://iconet-foundation.org/ns#",
  "@type": "InterpreterManifest",
  "@id": "/iconet/formats/markdown/manifest.json",
  "interpreters": [
    {
      "@id": "/iconet/formats/markdown/template.html",
      "sourceType": "application/neta+json",
      "targetType": "application/iconet+html",
      "sha-512": "<sha-512 hash>",
      "scripts": [
        "/iconet/formats/marked.min.js",
        "/iconet/formats/purify.min.js",
        "/iconet/formats/highlight.min.js",
        "/iconet/formats/tunnel.js"
      ],
      "permissions": {
        "content": true
      }
    },
    {
      "@id": "/iconet/formats/markdown/template.html",
      "sourceType": "text/markdown",
      "targetType": "application/iconet+html",
      "sha-512": "<sha-512 hash>",
      "scripts": [
        "/iconet/formats/marked.min.js",
        "/iconet/formats/purify.min.js",
        "/iconet/formats/highlight.min.js",
        "/iconet/formats/tunnel.js"
      ],
      "permissions": {
        "content": true
      }
    }
  ]
}